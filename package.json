{
  "name": "ikiosk",
  "version": "2.1.0",
  "private": true,
  "description": "Ikiosk Electron",
  "author": "Bryan Glennardy",
  "build": {
    "appId": "ikiosk",
    "extends": null,
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "public/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "extraResources": [
      {
        "from": "node_modules/regedit/vbs",
        "to": "regedit/vbs",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "public/",
        "to": "public/",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "homepage": "./",
  "main": "public/electron-main.js",
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@material-tailwind/react": "^2.1.8",
    "@reduxjs/toolkit": "^2.0.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "adm-zip": "^0.5.15",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.7",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "electron-is-dev": "^2.0.0",
    "electron-pos-printer": "^1.3.6",
    "lottie-react": "^2.4.0",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "os": "^0.1.2",
    "pdf-to-printer": "^5.6.0",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.4",
    "primereact": "^10.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-icons": "^4.12.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "react-scripts": "5.0.1",
    "react-simple-keyboard": "^3.7.63",
    "react-spinners": "^0.13.8",
    "redux": "^5.0.1",
    "regedit": "^5.1.3",
    "swiper": "^11.0.5",
    "util": "^0.12.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "react-start": "set PORT=7000 && react-scripts start --require postcss start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:7000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "concurrently": "^8.2.2",
    "electron": "^22.3.27",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2",
    "tailwindcss": "^3.4.4",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0"
  }
}
